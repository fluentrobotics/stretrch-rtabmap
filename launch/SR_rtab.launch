<launch>

    <!-- Launch the rtabmap.launch file with custom parameters -->

    <include file="$(find rtabmap_ros)/launch/rtabmap.launch">

        <arg name="rgb_topic" value="/camera/color/image_raw"/>

        <arg name="depth_topic" value="/camera/aligned_depth_to_color/image_raw"/>

        <arg name="camera_info_topic" value="/camera/color/camera_info"/>

        <arg name="depth_camera_info_topic" value="/camera/aligned_depth_to_color/camera_info"/>

        <arg name="stereo" value="false"/>

        <arg name="rgbd_sync" value="true"/>

        <arg name="rtabmap_viz" value="false"/>

        <arg name="rviz" value="false"/>

        <arg name="rviz_cfg" value="/home/hello-robot/Vins_fusion_ws/src/stretch_vins_pkg/SR_rtab_rviz.rviz"/>

        <arg name="args" value="--delete_db_on_start"/>

        <arg name="publish_tf_odom"          default="true"/>

